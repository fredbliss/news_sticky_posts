{
  "name": "intelligentspark/contao_news_sticky_posts",
  "description": "Add a sticky posts features to the news module",
  "keywords": [
    "contao",
    "frontend",
    "hook",
    "news",
    "sticky"
  ],
  "type": "contao-module",
  "license": "LGPL-3.0+",
  "require": {
    "php": ">=5.3.2",
    "contao/core": ">=3.3.0,<4.0.0-dev",
    "contao-community-alliance/composer-plugin": "*",
    "terminal42/contao-namespace-class-loader":"~1.0"
  },
  "authors": [
    {
      "name": "intelligentspark",
      "email": "fred.bliss@intelligentspark.com"
    }
  ],
  "autoload": {
    "psr-0": {
      "IntelligentSpark\\": [
        "system/modules/news_sticky_posts/library/"
      ]
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/news_sticky_posts"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  }
}